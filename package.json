{
  "name": "@moxy/next-sitemaps",
  "version": "3.1.0",
  "description": "Library to generate sitemaps in a next application",
  "main": "lib/index.js",
  "files": [
    "lib",
    "es",
    "plugin.js",
    "!**/*.test.js",
    "!**/__snapshots__",
    "!**/__mocks__",
    "!**/__fixtures__"
  ],
  "scripts": {
    "lint": "eslint . --ignore-path .gitignore",
    "test": "jest",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src -d lib --delete-dir-on-start",
    "build:es": "cross-env BABEL_ENV=es babel src -d es --delete-dir-on-start",
    "build": "npm run build:commonjs && npm run build:es",
    "prerelease": "npm t && npm run lint && npm run build",
    "release": "standard-version",
    "postrelease": "git push --follow-tags origin master && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moxystudio/next-sitemaps-plugin.git"
  },
  "keywords": [
    "create-next-sitemaps",
    "nextjs",
    "routes",
    "next-sitemaps"
  ],
  "author": "Pedro Santos <pedro.santos@moxy.studio>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moxystudio/next-sitemaps-plugin/issues"
  },
  "homepage": "https://github.com/moxystudio/next-sitemaps-plugin#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@commitlint/config-conventional": "^9.1.1",
    "@moxy/babel-preset": "^3.2.5",
    "@moxy/eslint-config-babel": "^12.0.1",
    "@moxy/eslint-config-base": "^12.0.1",
    "@moxy/eslint-config-jest": "^12.0.1",
    "@moxy/jest-config-base": "^5.0.0",
    "commitlint": "^9.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "husky": "^4.2.3",
    "jest": "^26.1.0",
    "lint-staged": "^10.0.8",
    "next": "^9.3.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "standard-version": "^8.0.2",
    "supertest": "^4.0.2"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "next": ">=8 <=9"
  },
  "dependencies": {
    "@hapi/boom": "^9.1.0",
    "glob": "^7.1.6",
    "lodash": "^4.17.19",
    "proper-url-join": "^2.1.1",
    "webpack-sources": "^1.4.3"
  }
}
